<h2 class="page-header">Account Login</h2>
<form method="post" action="/users/login">
  <div class="form-group">
    <label for="exampleInputUsername">Username</label>
    <input type="text" class="form-control" name="username" aria-describedby="usernameHelp" placeholder="Enter username">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" name="password" placeholder="Password">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>

</form>
<div class="col-xs-12 col-md-4 social-login">
  <div class="social-wrapper">

    <a href="auth/google">
      <div class="google">
        <div>
          <img src="../../images/glogin.png" alt="google" class="gplus-image" />
        </div>
        <div class="login-text">Continue with google</div>
      </div>
    </a>
  </div>
</div>
